{
    "aui-widget": {
        "use": [
            "aui-widget-cssclass",
            "aui-widget-toolbars"
        ]
    },

    "aui-widget-core": {
        "use": [
            "aui-widget-cssclass"
        ]
    },

    "aui-widget-cssclass": {
        "requires": [
            "widget-base"
        ]
    },

    "aui-widget-responsive": {
        "requires": [
            "event-resize",
            "widget-base"
        ]
    },

    "aui-widget-swipe": {
        "requires": [
            "classnamemanager",
            "scrollview-base",
            "scrollview-paginator",
            "timers"
        ]
    },

    "aui-widget-toolbars": {
        "requires": [
            "widget-stdmod",
            "aui-toolbar"
        ]
    },

    "aui-widget-toggle": {
    },

    "aui-widget-position-align-suggestion": {
        "requires": [
            "widget-position-align",
            "widget-stdmod"
        ]
    },

    "aui-widget-shortcut": {
        "requires": [
            "base"
        ]
    },

    "aui-widget-transition": {
        "requires": [
            "transition"
        ]
    },

    "aui-widget-trigger": {
        "requires": [
            "node"
        ]
    }
}
