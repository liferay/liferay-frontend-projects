{
	"author": "Liferay Frontend Infrastructure Team <pt-frontend-infrastructure@liferay.com>",
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "4.3.0",
		"@typescript-eslint/parser": "4.3.0",
		"eslint-config-prettier": "6.12.0",
		"eslint-plugin-no-for-of-loops": "^1.0.0",
		"eslint-plugin-no-only-tests": "^2.1.0",
		"eslint-plugin-notice": "0.9.10",
		"eslint-plugin-react": "^7.21.3",
		"eslint-plugin-react-hooks": "^4.1.2",
		"eslint-plugin-sort-destructure-keys": "^1.3.5"
	},
	"description": "ESLint shareable config for the Liferay JavaScript Style",
	"files": [
		".eslintrc.js",
		"index.js",
		"metal.js",
		"plugins",
		"portal.js",
		"react.js",
		"scripts",
		"utils"
	],
	"keywords": [
		"code",
		"eslint",
		"eslintconfig",
		"liferay",
		"quality"
	],
	"license": "MIT",
	"main": "index.js",
	"name": "@liferay/eslint-config",
	"peerDependencies": {
		"eslint": ">=4.1.1",
		"typescript": "*"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"repository": {
		"directory": "projects/eslint-config",
		"type": "git",
		"url": "https://github.com/liferay/liferay-frontend-projects.git"
	},
	"scripts": {
		"build": "true",
		"ci": "liferay-workspace-scripts ci",
		"format": "liferay-workspace-scripts format",
		"format:check": "liferay-workspace-scripts format:check",
		"lint": "liferay-workspace-scripts lint",
		"lint:fix": "liferay-workspace-scripts lint:fix",
		"postinstall": "node scripts/postinstall.js",
		"postversion": "liferay-workspace-scripts publish",
		"preversion": "liferay-workspace-scripts ci",
		"test": "liferay-workspace-scripts test"
	},
	"version": "23.0.0"
}
