		"format:check": "yarn run liferay-npm-scripts prettier --list-different \"**/*.{js,json,md,ts,yml}\" \"**/.*.{js,ts,yml}\"",
		"lint": "eslint \"**/*.{js,ts}\" \"**/.*.{js,ts}\"",
		"lint:fix": "eslint --fix \"**/*.{js,ts}\" \"**/.*.{js,ts}\"",
		"test": "jest"
	},
	"version": "0.0.1",
	"workspaces": [
		"projects/*",
		"projects/*/packages/*"
	]
