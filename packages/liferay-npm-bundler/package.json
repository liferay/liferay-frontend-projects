{
	"name": "liferay-npm-bundler",
	"version": "2.12.0",
	"description": "A CLI utility to bundle NPM dependencies of a Liferay OSGi bundle.",
	"main": "lib/index.js",
	"bin": {
		"liferay-npm-bundler": "bin/liferay-npm-bundler.js"
	},
