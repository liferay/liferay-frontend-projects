		"build": "babel --source-maps -D -d lib src --ignore '**/__tests__/**'",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-preset-es2015": "^6.24.1",
		"liferay-npm-bundler-preset-liferay-dev": "1.1.3"
	},
	"dependencies": {
		"babel-core": "^6.25.0",
		"clone": "^2.1.2",
		"data-urls": "^1.0.0",
		"dot-prop": "^5.0.0",
		"fs-extra": "^4.0.2",
		"globby": "^10.0.1",
		"insight": "0.10.0",
		"jszip": "^3.1.5",
		"liferay-npm-build-tools-common": "2.12.0",
		"liferay-npm-bundler-preset-standard": "2.12.0",
		"merge": "^1.2.1",
		"pretty-time": "^0.2.0",
		"read-json-sync": "^1.1.1",
		"resolve": "^1.3.3",
		"rimraf": "^2.6.3",
